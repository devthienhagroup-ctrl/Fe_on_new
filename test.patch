+      <div
+        v-for="message in messages"
+        :key="message.id"
+        class="flex items-start gap-3"
+      >
+        <div
+          class="w-10 h-10 rounded-2xl flex items-center justify-center text-white font-semibold shadow-lg"
+          :class="avatarColor(message.accent)"
+        >
+          {{ message.sender.split(' ').slice(-1)[0].charAt(0) }}
+        </div>
+        <div class="flex-1">
+          <div class="flex items-center justify-between">
+            <p class="font-semibold text-slate-800">
+              {{ message.sender }}
+              <span class="text-xs text-slate-400 font-normal">• {{ message.role }}</span>
+            </p>
+            <span class="text-xs text-slate-400">{{ message.time }}</span>
+          </div>
+          <div class="mt-2 bg-slate-50 border border-slate-100 rounded-2xl px-4 py-3 shadow-sm">
+            <p class="text-sm text-slate-600">{{ message.content }}</p>
+          </div>
+        </div>
+      </div>
+    </div>
+
+    <div class="flex items-center gap-3 rounded-2xl bg-slate-50 border border-slate-100 px-4 py-3 shadow-inner">
+      <input
+        type="text"
+        class="flex-1 bg-transparent outline-none text-sm text-slate-600"
+        placeholder="Nhập tin nhắn nội bộ hoặc theo ngữ cảnh hợp tác..."
+      />
+      <button
+        type="button"
+        class="px-4 py-2 rounded-xl bg-blue-600 text-white text-sm font-semibold shadow-md hover:-translate-y-0.5 hover:shadow-xl transition-all duration-200"
+      >
+        Gửi
+      </button>
+    </div>
+  </section>
+</template>
+
+<script setup>
+const props = defineProps({
+  chatContext: {
+    type: Object,
+    required: true,
+  },
+  messages: {
+    type: Array,
+    required: true,
+  },
+  teamName: {
+    type: String,
+    required: true,
+  },
+  productName: {
+    type: String,
+    default: "",
+  },
+});
+
+const avatarColor = (accent) => {
+  if (accent === "indigo") return "bg-indigo-500";
+  if (accent === "emerald") return "bg-emerald-500";
+  return "bg-blue-500";
+};
+</script>
