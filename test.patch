+<template>
+  <div class="min-h-screen bg-gradient-to-br from-slate-50 via-indigo-50 to-violet-100">
+    <div class="flex min-h-screen">
+      <TeamSidebar :active-menu="activeMenu" @update:activeMenu="activeMenu = $event" />
+      <div class="flex-1 flex flex-col gap-6 p-6">
+        <TeamHeader :notification-count="notificationCount" />
+
+        <section class="grid grid-cols-1 lg:grid-cols-4 gap-4">
+          <div class="bg-white rounded-2xl shadow-xl border border-slate-100 p-4">
+            <p class="text-sm text-slate-500">Tổng số team</p>
+            <p class="text-2xl font-semibold text-slate-800">{{ teams.length }}</p>
+          </div>
+          <div class="bg-white rounded-2xl shadow-xl border border-slate-100 p-4">
+            <p class="text-sm text-slate-500">Sản phẩm đang quản lý</p>
+            <p class="text-2xl font-semibold text-slate-800">{{ products.length }}</p>
+          </div>
+          <div class="bg-white rounded-2xl shadow-xl border border-slate-100 p-4">
+            <p class="text-sm text-slate-500">Yêu cầu chờ duyệt</p>
+            <p class="text-2xl font-semibold text-amber-500">{{ pendingRequests }}</p>
+          </div>
+          <div class="bg-white rounded-2xl shadow-xl border border-slate-100 p-4">
+            <p class="text-sm text-slate-500">Tin nhắn chưa đọc</p>
+            <p class="text-2xl font-semibold text-blue-600">{{ unreadMessageCount }}</p>
+          </div>
+        </section>
+
+        <main class="flex-1">
+          <div v-if="activeMenu === 'teams'" class="grid gap-6">
+            <div class="bg-white rounded-2xl shadow-xl border border-slate-100 p-6">
+              <h3 class="text-xl font-semibold text-slate-800">Danh sách Team</h3>
+              <p class="text-sm text-slate-500">Saler tạo và điều phối team xử lý sản phẩm.</p>
+            </div>
+            <div class="grid grid-cols-1 lg:grid-cols-3 gap-5">
+              <div
+                v-for="team in teams"
+                :key="team.id"
+                class="bg-white rounded-2xl shadow-xl border border-slate-100 p-5 hover:-translate-y-0.5 hover:shadow-2xl transition-all duration-200"
+              >
+                <div class="flex items-center justify-between">
+                  <h4 class="text-lg font-semibold text-slate-800">{{ team.name }}</h4>
+                  <span class="text-xs font-semibold bg-indigo-100 text-indigo-700 px-2 py-1 rounded-full">
+                    {{ team.performance }}
+                  </span>
+                </div>
+                <div class="mt-4 space-y-2 text-sm text-slate-600">
+                  <p><span class="font-semibold text-slate-700">Lead:</span> {{ team.lead }}</p>
+                  <p><span class="font-semibold text-slate-700">Thành viên:</span> {{ team.members }}</p>
+                  <p><span class="font-semibold text-slate-700">Chuyên môn:</span> {{ team.focus }}</p>
+                </div>
+                <div class="mt-4 flex items-center gap-2">
+                  <span class="text-xs font-semibold bg-blue-100 text-blue-700 px-2 py-1 rounded-full">Chat nội bộ</span>
+                  <span class="text-xs font-semibold bg-emerald-100 text-emerald-700 px-2 py-1 rounded-full">Nhiệm vụ</span>
+                </div>
+              </div>
+            </div>
+          </div>
+
+          <div v-else-if="activeMenu === 'collaborations'">
+            <CollaborationList :requests="requests" :products="products" :teams="teams" />
+          </div>
+
+          <div v-else class="grid grid-cols-1 lg:grid-cols-3 gap-6">
+            <div class="space-y-4">
+              <div
+                v-for="context in chatContexts"
+                :key="context.id"
+                class="bg-white rounded-2xl shadow-xl border border-slate-100 p-4 cursor-pointer hover:-translate-y-0.5 hover:shadow-2xl transition-all duration-200"
+                :class="activeChatId === context.id ? 'ring-2 ring-indigo-300' : ''"
+                @click="activeChatId = context.id"
+              >
+                <div class="flex items-center justify-between">
+                  <p class="font-semibold text-slate-800">{{ context.title }}</p>
+                  <span v-if="context.unread" class="text-xs font-semibold bg-red-500 text-white rounded-full px-2 py-0.5 shadow-md">
+                    {{ context.unread }}
+                  </span>
+                </div>
+                <p class="text-xs text-slate-400 mt-2">{{ context.subtitle }}</p>
+                <p class="text-sm text-slate-600 mt-2">
+                  {{ getTeam(context.teamId)?.name }}
+                  <template v-if="context.type === 'collaboration'">
+                    <span class="text-slate-300">•</span>
+                    {{ getProduct(context.productId)?.name }}
+                  </template>
+                </p>
+              </div>
+            </div>
+            <div class="lg:col-span-2">
+              <ChatBox
+                :chat-context="activeChatContext"
+                :messages="activeMessages"
+                :team-name="getTeam(activeChatContext.teamId)?.name || ''"
+                :product-name="activeChatContext.type === 'collaboration' ? getProduct(activeChatContext.productId)?.name || '' : ''"
+              />
+            </div>
+          </div>
+        </main>
+      </div>
+    </div>
+  </div>
+</template>
+
+<script setup>
+import { computed, ref } from "vue";
+import TeamSidebar from "./sidebar/TeamSidebar.vue";
+import TeamHeader from "./header/TeamHeader.vue";
+import CollaborationList from "./collaborations/CollaborationList.vue";
+import ChatBox from "./chat/ChatBox.vue";
+import {
+  mockTeams,
+  mockProducts,
+  mockRequests,
+  mockChatContexts,
+  mockMessages,
+} from "./mock/mockState";
+
+const activeMenu = ref("collaborations");
+const activeChatId = ref(mockChatContexts[0].id);
+
+const teams = ref(mockTeams);
+const products = ref(mockProducts);
+const requests = ref(mockRequests);
+const chatContexts = ref(mockChatContexts);
+const unreadMessageCount = ref(5);
+
+const pendingRequests = computed(
+  () => requests.value.filter((request) => request.status === "pending").length
+);
+
+const notificationCount = computed(() => pendingRequests.value + unreadMessageCount.value);
+
+const getTeam = (id) => teams.value.find((item) => item.id === id);
+const getProduct = (id) => products.value.find((item) => item.id === id);
+
+const activeChatContext = computed(
+  () => chatContexts.value.find((context) => context.id === activeChatId.value) || chatContexts.value[0]
+);
+
+const activeMessages = computed(() => mockMessages[activeChatContext.value.id] || []);
+</script>
